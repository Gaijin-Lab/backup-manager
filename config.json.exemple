{
  "repoPath": "/var/backups/backup-manager",
  "sources": [
    "/srv/projects/example-project"
  ],
  "restorePath": "/var/backups/backup-manager/restore",
  "ignore": [
    "**/.git/**",
    "**/node_modules/**",
    "**/.cache/**",
    "**/dist/**",
    "**/build/**",
    "**/run.sh",
    "**/default/**",
    "**/admins.json",
    "**/cache/**"
  ],
  "retentionDays": 15,
  "debounceSeconds": 10
}
